{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">회원 아이디</th>
        <th scope="col">비밀번호</th>
        <th scope="col">이름</th>
        <th scope="col">나이</th>
        <th scope="col">전화번호</th>
        <th scope="col">이메일</th>
    </tr>
    </thead>
    <tbody>
    {{#userList}}
        <tr>
            <th>{{id}}</th>
            <td><a href="/users/{{id}}">{{userId}}</a></td>
            <td>{{password}}</td>
            <td>{{name}}</td>
            <td>{{age}}</td>
            <td>{{phoneNumber}}</td>
            <td>{{email}}</td>
        </tr>
    {{/userList}}
    </tbody>
</table>

{{>layouts/footer}}
